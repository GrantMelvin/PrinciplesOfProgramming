    "def capitalizeImperative(lst):\n",
    "    reslst = []\n",
    "    for x in range(len(lst)):\n",
    "        if(x == 0):\n",
    "            reslst.append(lst[0].upper())\n",
    "        else:\n",
    "            reslst.append(lst[x].lower())\n",
    "    return ''.join(reslst)"
   ]
