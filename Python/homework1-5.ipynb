    "def capitalizeComprehension(wrd):\n",
    "    newLst = []\n",
    "    for x in range(0,len(wrd)):\n",
    "        if x == 0:\n",
    "            newLst.append(wrd[x].upper())\n",
    "        else:\n",
    "            newLst.append(wrd[x].lower())\n",
    "    return ''.join(newLst)"
   ]
